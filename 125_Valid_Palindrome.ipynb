    "class Solution:\n",
    "    def isPalindrome(self, s: str) -> bool:\n",
    "        string = \"\"\n",
    "        for i in s:\n",
    "            if i.isalnum():\n",
    "                string += i.upper()\n",
    "        if string[::-1] == string:\n",
    "            return True\n",
    "        else:\n",
    "            return False\n"
   ]
  }
