    "class Solution:\n",
    "    def mySqrt(self, x: int) -> int:\n",
    "        if x == 0:\n",
    "            return 0\n",
    "        else:\n",
    "            for i in range(x+1):\n",
    "                if i*i == x:\n",
    "                    return i\n",
    "                elif i*i < x and (i+1)*(i+1) > x:\n",
    "                    return i"
   ]
  }
